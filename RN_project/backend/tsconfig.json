{
  "compilerOptions": {
    "target": "es2016",
    "lib": ["es6","dom"],
    "experimentalDecorators": true,
    "module": "commonjs",
    "rootDir": "src",
    "resolveJsonModule": true,
    "outDir": "build",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  }
}
